STATIC_LIB = \
	freeimage_static.lib

DYNAMIC_LIB = \
	freeimage3.dll

LIBRARIES = \
	$(STATIC_LIB) \
	$(DYNAMIC_LIB)
		
!INCLUDE $(TOPDIR)\makefiles\nmake\sub.mk

freeimage3.dll: $(DYNAMIC_OBJS)
	$(LINK) /dll /out:$@ $(LDFLAGS) $(LIBS) $?

freeimage_static.lib: $(OBJS)
	$(LINK) /lib /out:$@ $(STATIC_LDFLAGS) $(LIBS) $?

local_all: $(LIBRARIES)

# clean locally and recursively, the standard clean is a little
# bit weak
local_clean:
	-@del /s *.obj >NUL 2>NUL
	-@del /s *.dllobj >NUL 2>NUL

local_distclean:

local_test:
